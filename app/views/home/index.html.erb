<div class="container">
  <h1>Welcome to your Idea Box!</h1>

  <div class="addIdea" id="addIdea">
    Add Idea here: <form class="form-inline addIdeaForm">
      <div class="form-group">
        <input type="text_field" id="title" placeholder="Idea Title" class="form-control">
      </div>
      <div class="form-group">
        <input type="text_field" id="body" placeholder="Idea Body" class="form-control">
      </div>
      <div class="form-group">
        <button type="submit" class="btn btn-success btn-md addIdea"  value="save" id="add">Save</button>
      </div>
    </form>
  </div><br>
  <div class="ideas">
    <table class="table ideas-table">
      <thead>
        <tr>
          <th>Title</th>
          <th>Body</th>
          <th>Quality</th>
          <th>Delete</th>
        </tr>
      </thead>
    </table>
  </div>
</div>
