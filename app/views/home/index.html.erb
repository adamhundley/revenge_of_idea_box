<div class="container text-center">
  <h1>Welcome to your Idea Box!</h1>
  <div class="row">
    <div class="lightbulbContainer col-sm-3">
      <img src="https://writtenbutnevershared.files.wordpress.com/2013/11/idea-lightbulb-sketch.jpg" class="lightbulb" alt="" />
    </div>
    <div class="addIdea col-sm-6" id="addIdea">
      <form class="form addIdeaForm">
        <div class="form-group">
          <input type="text_field" id="title" placeholder="Idea Title" class="form-control" required>
        </div>
        <div class="form-group">
          <textarea rows="5" cols="40" type="text_field" id="body" placeholder="Idea Body" class="form-control" required></textarea>
        </div>
        <div class="form-groupt">
          <button type="submit" class="btn btn-success btn-sm addIdea"  value="save" id="add">Save Idea</button>
        </div>
      </form>
    </div>
  <div class="lightbulbContainer col-sm-3">
    <img src="https://writtenbutnevershared.files.wordpress.com/2013/11/idea-lightbulb-sketch.jpg" class="lightbulb" alt="" />
  </div>
  </div>
  <br>
  <hr>
  <div class="searchIdea" id="searchIdea">
    <form class="form-inline searchIdeaForm">
      <div class="form-group">
        <input type="text_field" size="80" id="search" placeholder="Search" class="form-control" required>
      </div>
      <div class="form-group">
        <button type="submit" class="btn btn-info btn-md"  value="clear" id="clear">Clear</button>
      </div>
    </form>
  </div>
  <hr>
  <div class="ideas">
    <table class="table text-center table-responsive ideas-table">
      <thead>
        <tr>
          <th class="text-center">Title</th>
          <th class="text-center">Body</th>
          <th class="text-center">Quality</th>
          <th class="text-center">Vote</th>
          <th class="text-center">Delete</th>
        </tr>
      </thead>
    </table>
  </div>
</div>
<hr>
